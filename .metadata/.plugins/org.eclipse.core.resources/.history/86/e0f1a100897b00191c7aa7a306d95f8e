package com.app.dao;

import java.util.ArrayList;
import java.util.List;

import org.springframework.stereotype.Service;

import com.app.model.Customer;

@Service
public class CustomerDaoImpl implements CustomerDao{

	@Override
	public List<Customer> getCustomer() {
		
		List<Customer> empList=new ArrayList<Customer>();
		
		Customer emp1=new Customer(532,"Narasimha","Hyd");
		Customer emp2=new Customer(532,"Narasimha","Hyd");
		Customer emp3=new Customer(532,"Narasimha","Hyd");
		empList.add(emp1);
		empList.add(emp2);
		empList.add(emp3);
		// TODO Auto-generated method stub
		return empList;
	}

	@Override
	public List<Customer> getAllCustomer() {
List<Customer> empList=new ArrayList<Customer>();
		
		Customer cust1=new Customer(532,"Narasimha","Hyd");
		Customer cust2=new Customer(532,"Narasimha","Hyd");
		Customer cust3=new Customer(532,"Narasimha","Hyd");
		empList.add(cust1);
		empList.add(cust2);
		empList.add(cust3);
		// TODO Auto-generated method stub
		return empList;	}

}
